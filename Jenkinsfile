    steps {
      echo "code quality"
   }
  }
  stage('dependency check') {
    steps {
      echo "Depe"
   }
      
  }
  stage('deploy') {
  steps {
      echo "code deploy"
   }
  }
  stage('email notification') {
  steps {
      echo "email"
   }
  }
  }
}
